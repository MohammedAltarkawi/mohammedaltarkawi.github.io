<mvc:View
	controllerName="mst.githubpage.controller.Main"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns:l="sap.ui.layout"
	xmlns:tnt="sap.tnt"
	core:require="{
		formatter: 'mst/githubpage/model/formatter'
	}">

	<Page showHeader="true">
		<headerContent>
			<Switch  change=".toggleTheme"/>
		</headerContent>


			<footer class="mstPageFooter">
				<OverflowToolbar id="klab" ariaHasPopup="Dialog" class="mstPageFooter">
					<layoutData>
						<OverflowToolbarLayoutData priority="AlwaysOverflow"/>
					</layoutData>
					<ToolbarSpacer/>
					<Button icon="sap-icon://lightbulb" ariaHasPopup="Dialog" type="Emphasized" width="5px" press="onOpenDialog" class="roundClass"/>
				</OverflowToolbar>
			</footer>
	</Page>
	

</mvc:View>
